#!/home/ub/click/click/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'HelloWorld==1.0','console_scripts','hello'
__requires__ = 'HelloWorld==1.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('HelloWorld==1.0', 'console_scripts', 'hello')()
    )
